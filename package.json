{
  "name": "nexart-design-studio",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev -turbo",
    "build": "next build -turbo",
    "start": "next start",
    "test": "echo 'No tests configured' | tee _devlogs/test-$(date +%Y%m%d-%H%M%S).log",
    "check-types": "tsc --noEmit 2>&1 | tee _devlogs/typecheck-$(date +%Y%m%d-%H%M%S).log",
    "lint": "next lint 2>&1 | tee _devlogs/lint-$(date +%Y%m%d-%H%M%S).log",
    "lint:fix": "next lint --fix 2>&1 | tee _devlogs/lint-fix-$(date +%Y%m%d-%H%M%S).log",
    "format": "prettier --write . 2>&1 | tee _devlogs/format-$(date +%Y%m%d-%H%M%S).log",
    "format:check": "prettier --check . 2>&1 | tee _devlogs/format-check-$(date +%Y%m%d-%H%M%S).log",
    "validate": "pnpm check-types && pnpm lint && pnpm format:check",
    "capabilities": "tsx scripts/generate-capabilities.ts",
    "export:nexart": "tsx scripts/export-nexart.ts"
  },
